{
  "requiresCompatibilities": [
      "FARGATE"
  ],
  "family": "LearnJenkinsApp-TaskDefinition-Prod",
  "containerDefinitions": [
      {
          "name": "learn-jenkins-app",
          "image": "nginx:1.27-alpine",
          "portMappings": [{
              "name": "nginx-80-tip",
              "containerPort": 80,
              "hostPort": 80,
              "protocol": "tcp",
              "appProtocol": "http"
          }],
          "essential": true
      }
  ],
  "volumes": [],
  "networkMode": "awsvpc",
  "memory": "512",
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::054037114950:role/ecsTaskExecutionRole"
}